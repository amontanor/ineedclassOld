            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dotidapp.ineedclass.Registro"
            android:label="@string/titler"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dotidapp.ineedclass.DatosProfesor"
            android:label="@string/titler"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dotidapp.ineedclass.NuevoBuscador"
            android:label="@string/title_activity_nuevo_buscador"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="dotidapp.ineedclass.DatosAcademia"
            android:label="@string/title_activity_datos_academia"
            android:screenOrientation="portrait" >
